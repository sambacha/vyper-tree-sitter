{
  "name": "tree-sitter-vyper",
  "version": "0.0.5",
  "description": "EVM Pythonic Smart Contract Language",
  "repository": "https://github.com/vyperlang",
  "license": "MIT",
  "author": {
    "name": "Sam Bacha"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "vyper"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "@types/node": "^22.17.1",
    "commander": "^14.0.0",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "web-tree-sitter": "^0.25.8"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
